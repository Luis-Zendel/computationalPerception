   "source": [
    "import cv2 as cv\n",
    "img = cv.imread('coche11.jpeg')\n",
    "b,g,r = cv.split(img)\n",
    "b_eq = cv.equalizeHist(b)\n",
    "g_eq = cv.equalizeHist(g)\n",
    "r_eq = cv.equalizeHist(r)\n",
    "img_eq = cv.merge((b_eq, g_eq, r_eq))\n",
    "cv.imshow('Equalizada', img_eq)\n"
   ]
  }
